{
    "manifest_version": 2,
    "name": "Summ",
    "description": "Research Assistant",
    "version": "1.0",
    "permissions": [
        "tabs", "http://*/*", "https://*/*"
    ],
    "browser_action": {
        "default_title": "Set this page's color.",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "css": ["styles.css"],
            "js": ["content.js", "./js/jquery/jquery.min.js"]
        }
    ]
}